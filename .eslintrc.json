{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [],
  "plugins": ["unused-imports", "react-hooks"],
  "ignorePatterns": ["lib/tradingview/*"],
  "rules": {
    "react-hooks/exhaustive-deps": "warn"
  }
}
